{
    "manifest_version": 3,
    "name": "Altify",
    "description": "AI Alt Text",
    "version": "1.0",
    "icons":{
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "action": {
      "default_icon": "/images/icon128.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "permissions": [
      "activeTab",
      "contextMenus",
      "storage"
    ]    

  }




  